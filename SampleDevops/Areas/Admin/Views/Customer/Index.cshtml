<!--OpenCatapultModelId:3-->
@model List<SampleDevops.Areas.Admin.Models.CustomerViewModel>
@{
    ViewData["Title"] = "View Customer";
}
<h2>@ViewData["Title"]</h2>
<div>
    <a asp-area="Admin" asp-controller="Customer" asp-action="Create" class="btn btn-default">Create</a>
</div>
<div>&nbsp;</div>
<table class="table">
    <tr>
        <th>Name</th>
        <th></th>
    </tr>
@foreach (var item in Model)
{
    <tr>
        <td>@item.Name</td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
        </td>
    </tr>
}
</table>
