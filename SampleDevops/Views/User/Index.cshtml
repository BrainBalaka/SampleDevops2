@model List<SampleDevops.Models.UserViewModel>
@{
    ViewData["Title"] = "View User";
}
<h2>@ViewData["Title"]</h2>
<div>&nbsp;</div>
<table class="table">
    <tr>
        <th>User name</th>
        <th>Email</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Is Active?</th>
    </tr>
@foreach (var item in Model)
{
    <tr>
        <td>@item.UserName</td>
        <td>@item.Email</td>
        <td>@item.FirstName</td>
        <td>@item.LastName</td>
        <td>@item.IsActive</td>
    </tr>
}
</table>
